#sticky-wrapper
  width 100%
  position relative
  height 200000
  background #fbf8f8
  .wrapper-header
    width 100%
    height 70px
    background #fff
    position absolute
    top 0
    left 0
    right 0
    z-index 10
    .header-content
      margin 0 auto
      width 87%
      height 100%
      display flex
      justify-content space-between
      align-items center
      .header-logo
        width: 280px
        height 40px
      .header-nav
        height 100%
        width 70%
        display flex
        justify-content space-between
        align-items center
        line-height 100%
        .nav-address
          display flex
          flex 1
          a
            img
              display block
          .address-city
            font-size 17px
            color #121212
            font-weigh bold
          .switch-city
            font-size 17px
            color #121212
            font-weigh bold
            padding-left 5px
        .nav-menu
          flex 3
          .menu-list
            display flex
            justify-content: space-between
            .menu-item
              a
                font-size 16px
                font-weight bold
                color #121212
                transition color .5s
              a:hover
                color red
              .shouye
                color red
              .login
                color #fff
                background red
                padding 2px 3px
                border-radius 3px
              .login:hover
                color #fff
  .wrapper-content
    width 100%
    position relative
    padding-top 71px
    .swiper-container
      position relative
      .swiper-wrapper
        height 100%
        .swiper-slide
          height 420px
          background-size cover
          background-position 50% 50%
        .s1
          background-image url("http://www.daoway.cn/pcimages/banner1.jpg")
        .s2
          background-image url("http://www.daoway.cn/pcimages/banner2.jpg")
        .s3
          background-image url("http://www.daoway.cn/pcimages/banner3.jpg")
      .swiper-pagination-bullet
        background #7e8c8d
        width 10px
        height 10px
      .swiper-pagination-bullet-active
        background #fff
        width 10px
        height 10px
      .menu-nav
        width 180px
        height 420px
        background #757881
        position absolute
        left 8%
        top 0
        z-index 100
        .nav-list
          width 100%
          .nav-item
            width 100%
            height 42px
            .item-pos
              width 100%
              height 100%
              text-align center
              line-height 42px
              box-sizing border-box
              position relative
              a
                width 100%
                height 100%
                color #fff
                font-weight bold
                margin-right 5px
            .firstImg
              width 6px
              height 11px
              position absolute
              right 35px
              top 18px
              display block
            .secondImg
              width 6px
              height 11px
              position absolute
              right 0
              top 0
              bottom 0
              margin auto
              display none
            .nextMenu
              width 160px
              height 420px
              background #fff
              position absolute
              left 180px
              top 0
              display none
              .nextMenu-item
                width 100%
                height 42px
                box-sizing border-box
                a
                  display block
                  width 100%
                  height 100%
                  line-height 42px
                  font-weight bold
                  color #121212
                  text-align left
                  padding-left 45px
          .nav-item:hover
            background #60646d
            .firstImg
              display none
            .secondImg
              display block
            .nextMenu
              width 160px
              height 420px
              background #fff
              position absolute
              left 180px
              top 0
              display block
    .servicebody
      width 100%
      .serviceList
        width 100%
        padding-top 50px
        background #fff
        .service-title
          color #121212
          font-size 30px
          font-weight bold
          text-align center
        .service-type
          margin-top 15px
          text-align center
          box-sizing border-box
          position relative
          a
            font-size 22px
            color #3d3838
            padding 0 5px
            font-family "幼圆"
            transition color .5s
          a:hover
            color red
          .more-server
            position absolute
            right 6%
            color #3d3838
            font-size 20px
        .shopList
          width 87%
          margin 30px auto 0
          display flex
          justify-content space-between
          a
            display block
            margin-right 5px
            .shopItem
              display flex
              flex-direction column
              align-items flex-start
              padding 10px
              margin 0 auto
              box-sizing border-box
              border 1px solid #eaeaea
              background #fff
              img
                width 210px
                height 210px
              .serviceName
                font-size 16px
                font-weight 800
                padding 10px 0
                color #000
                white-space nowrap
                text-overflow ellipsis
              .description
                color #929292
                font-size 13px
                padding-bottom 15px
              .price
                display flex
                justify-content space-between
                width 100%
                .price-bottom
                  .price-detail
                    font-size 20px
                    line-height 20px
                    color red
                  .price-text
                    font-size 16px
                    color red
                    padding-right 5px
                .btn
                  margin-right 15px
                  font-size 14px
                  background #fb474f
                  color #fff
                  width 76px
                  height 24px
                  text-align center
                  line-height 24px
                  border-radius 5px
            .shopItem:hover
              box-shadow 5px 5px 15px #ccc
      .listTwo
        background none
    .baozhang
      width 100%
      height 148px
      background #fff
      padding-top 60px
      text-align center
      .baozhang-title
        font-size: 30px;
        font-family: "微软雅黑";
        font-weight: 800;
        span
          color #fb474f
          font-size: 30px;
          font-family: "微软雅黑";
          font-weight: 800;
      p
        font-size: 22px;
        color: #3d3838;
        padding-top 10px
      .servi
        padding-top 20px
        .servi-list
          width 100%
          display flex
          justify-content center
          .servi-item
            display flex
            justify-content center
            margin-right 25px
            img
              display block
            span
              padding-left 3px
              font-size 18px
    .footerContainer
      width 100%
      background #ffffff
      height 120px
      padding-top 50px
      .footer-city
        margin 0 auto
        width 87%
        display flex
        .hotCity
          flex 1
          font-size 19px
          font-weight 800
        .citiesName
          flex 9
          display flex
          justify-content space-around
          width 100%
          flex-wrap wrap
          align-content space-between
          a
            flex 0 0 9%
            width 9%
            color #898989
            font-size 16px
            font-weight 200
            font-family "微软雅黑"
            height 30px
    .cooperation
      width 87%
      padding-top 30px
      margin 0 auto
      .cooperation-title
        font-size 18px
        font-weight bold
        color #1d1515
      .brand
        width 100%
        .brand-list
          width 100%
          display flex
          justify-content space-between
          flex-wrap wrap
          margin 0 auto
          .brand-item
            height 60px
            flex 0 0 10%
            background #fff
            text-align center
            line-height 60px
            margin-right 1%
            margin-top 20px
            img
              width 100px
              height 47px
              vertical-align middle
    .copy
      width 87%
      margin 0 auto
      height 40px
      line-height 40px
      color #898989
      font-size 16px
